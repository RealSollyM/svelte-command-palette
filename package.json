{
	"name": "svelte-command-palette",
	"version": "1.1.7",
	"scripts": {
		"build:docs": "node generateDocSnippets.js",
		"dev": "npm run build:docs && svelte-kit dev",
		"build": "npm run build:docs && svelte-kit build",
		"package": "svelte-kit package && cp ./README.md ./package/README.md",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"release": "release-it",
		"pre:package-release": "rm -rf ./package"
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"release-it": "^15.1.1",
		"shiki": "^0.10.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.3",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"csstype": "^3.1.0",
		"fuse.js": "^6.6.2",
		"i": "^0.3.7",
		"npm": "^8.10.0",
		"svelte-command-palette": "^1.1.4",
		"svelte2tsx": "^0.5.10",
		"tinykeys": "^1.4.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"svelte",
		"svelte-command-palette",
		"command-palette",
		"cmd-palette",
		"palette",
		"sveltejs",
		"sveltejs command palette"
	],
	"contributors": [
		{
			"email": "rohit.mmm1996@gmail.com",
			"name": "Rohit Kashyap",
			"url": "https://rohitpotato.vercel.app/"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rohitpotato/svelte-command-palette.git"
	},
	"homepage": "https://svelte-command-palette.vercel.app/"
}
